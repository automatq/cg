{
    "version": 2,
    "routes": [
        {
            "src": "/api/content",
            "dest": "/api/content.js"
        },
        {
            "src": "/api/upload",
            "dest": "/api/upload.js"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}